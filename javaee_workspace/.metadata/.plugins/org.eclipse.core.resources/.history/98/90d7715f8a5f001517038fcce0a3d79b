    #include &ltiostream>
    using namespace std;

    int main(){
        int n;
        int sum;
        int m;
        while(cin>>n){
            sum = 0;
            for(int i = 0; i < n; i++){
                cin>>m;
                sum += m;   
            }
            if(sum%4 == 0){
                cout&lt&ltsum/4&lt&ltendl;
            }else{
                cout&lt&lt(sum/4)+1&lt&ltendl;
            }
        }
    }
